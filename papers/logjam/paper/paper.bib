@article{BlanchetAbadiFournetJLAP07,
  author = 	 {B. Blanchet and M. Abadi and C. Fournet},
  title = 	 {Automated Verification of Selected Equivalences for Security Protocols},
  LONGjournal = "Journal of Logic and Algebraic Programming",
  journal = "JLAP",
  volume = 75,
  number = 1,
  LONGpages = "3--51",
  year = 	 2008,
  dx = "http://dx.doi.org/10.1016/j.jlap.2007.06.002",
  filename = "automated-verification-of-selected-equivalences-for-security-protocols-jlap2007.pdf",
  abstract = "In the analysis of security protocols, methods and tools for
reasoning about protocol behaviors have been quite effective.  We aim
to expand the scope of those methods and tools.  We focus on proving
equivalences P ~ Q in which P and Q are two processes that differ only
in the choice of some terms.  These equivalences arise often in
applications.  We show how to treat them as predicates on the
behaviors of a process that represents P and Q at the same time.  We
develop our techniques in the context of the applied pi calculus and
implement them in the tool ProVerif."
}
