@INPROCEEDINGS{jim01sd3,
    Author         = {Trevor Jim},
    Booktitle      = {IEEE Symposium on Security and Privacy},
    Title          = {{SD3}: A Trust Management System with Certified
                      Evaluation},
    Year           = {2001},
    Pages          = {106--115},
    Abstract       = {SD3 is a successor to the QCM trust management
                      system. In SD3, policies are written in datalog
                      (similar to Prolog) extended with SDSI global
                      names. SD3 extends QCM with recursive policies and a
                      certified evaluator which produces a proof that the
                      answer to a query follows from the security policy
                      rules. The proof is checked by a very simple
                      checker, thus reducing the trusted computing
                      base. SD3 authorisation is based on SDSI style
                      public key bindings. Thus it is capability-based,
                      not role- or property-based and is not so well
                      suited for establishing trust between complete
                      strangers.}
}

@INPROCEEDINGS{li02rolebased,
    Author         = {Ninghui Li and John C. Mitchell and William
                      H. Winsborough},
    Booktitle      = {Proceedings of the 2002 IEEE Symposium on Security
                      and Privacy},
    Pages          = {114--130},
    Title          = {Design of a Role-Based Trust Management Framework},
    Year           = {2002},
    Abstract       = {This paper introduces various extensions of RT0, the
                      simplest attribute-based, role-based trust
                      management language of the RT family. All the
                      extensions are given semantics by translation into
                      safe datalog rules. RT1 supports parameterised roles
                      with typed parameters. RT2 introduces the notion of
                      o-sets, which are used to group logically related
                      objects (similar to action parameters). RTT supports
                      manifold roles. Now members of a role are sets of
                      entities, not just single entities. This is used for
                      policies where several entities have to get together
                      to perform one action. There is also a special
                      operator for saying that all these entities must be
                      disjunct. It is however not possible to express
                      mutual exclusion of roles, so not all separation of
                      duty constraints are expressible. Finally, RTD
                      supports delegation of role activations (a bit
                      unclear). }
}

